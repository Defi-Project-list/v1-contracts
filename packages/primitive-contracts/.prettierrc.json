{
  "overrides": [
    {
      "files": ["*.sol", "*.js"],
      "options": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": false,
        "bracketSpacing": true,
        "explicitTypes": "always",
        "jsxBracketSameLine": false,
        "semi": true
      }
    }
  ]
}
